{
  "name": "@tailor-cms/garment",
  "type": "module",
  "version": "0.18.0",
  "packageManager": "pnpm@9.12.2",
  "author": "Damir P. <underscope@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/underscope/garment.git"
  },
  "bugs": "https://github.com/underscope/garment/issues",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "bytes": "^3.1.2",
    "camelcase": "^8.0.0",
    "class-transformer": "^0.5.1",
    "mime-types": "^2.1.35",
    "object-sizeof": "^2.6.5",
    "reflect-metadata": "^0.2.2",
    "set-value": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/bytes": "^3.1.4",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.7.7",
    "bumpp": "^9.7.1",
    "tsup": "^7.2.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "target": [
      "node14",
      "es2022"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "bundle": true,
    "minify": false,
    "clean": false,
    "dts": true
  }
}
